   "source": [
    "SELECT\n",
    "  state_abbr,\n",
    "  SUM(CAST(REPLACE(`GHG emissions mtons CO2e`,\",\",\"\") AS DOUBLE)) as total_emissions\n",
    "FROM\n",
    "  test_sakthi.my_schema.emissions_data\n",
    "group by \n",
    "  state_abbr\n",
    "order by \n",
    "  total_emissions desc  \n",
    "LIMIT 10;"
   ]
